<li>
<span class="title"><%= link_to entry.title, entry%></span>
<span class="content"><%= entry.content[0..100] + '...' %></span>
<span class="timestamp">
Posted: <%= entry.created_at %> .
</span>
<% if current_user?(entry.user) %>
<%= link_to "delete", entry, method: :delete, confirm: "Are you sure?", title: entry.title %>
<% end %>
</li>


